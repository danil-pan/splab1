names=($(cat log.txt | cut -f12 -d" " |cut -f1 -d"/"| grep -v "-"|grep -v "<a" | sort | uniq -c | sort -n | sort -nr))
sum=0
for ((i=1;i<21;i=i+2));do
	sum=$((${sum}+${names[i-1]}))
done
for ((i=1;i<21;i=i+2));do
	var="${names[i]:1}"
	echo $var-${names[i-1]} - $(($((${names[i-1]}))*100/${sum} ))%
done
